<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @exercise.name %>
</p>
<ul>
	<% @exercise.turns.each do |turn| %>
	<li>
	<%= "Set Number: #{turn.turn_num}" %>
	<%= "Reps: #{turn.reps}" %>
	<%= "Weight: #{turn.weight}" %>
	</li>
	<% end %>
	
	
</ul>

<%= link_to 'Edit', edit_exercise_path(@exercise) %> |
<%= link_to 'Back', exercises_path %>
